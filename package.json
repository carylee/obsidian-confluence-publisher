{
    "name": "obsidian-confluence-publisher",
    "version": "1.0.1",
    "description": "This library allows you to publish your notes to Confluence",
    "main": "main.js",
    "type": "module",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "node esbuild.config.mjs production",
        "typecheck": "tsc --noEmit",
        "fmt": "npx prettier --write src/",
        "lint": "eslint --ignore-path ../../.eslintignore --ext .jsx,tsx,.js,.ts src/",
        "prettier-check": "npx prettier --check src/"
    },
    "keywords": [],
    "author": "andymac4182",
    "license": "Apache 2.0",
    "devDependencies": {
        "@types/mime-types": "^2.1.4",
        "@types/react": "^16.14.65",
        "@types/react-dom": "^16.9.25",
        "builtin-modules": "^5.0.0",
        "csstype": "^3.1.3",
        "dotenv": "^17.0.1",
        "esbuild": "^0.25.5",
        "obsidian": "1.1.1"
    },
    "dependencies": {
        "@markdown-confluence/lib": "5.5.2",
        "@markdown-confluence/mermaid-electron-renderer": "5.5.2",
        "confluence.js": "^1.7.4",
        "mermaid": "9.3.0",
        "mime-types": "^2.1.35",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "typescript": "4.9.5"
    },
    "resolutions": {
        "prosemirror-model": "1.14.3"
    }
